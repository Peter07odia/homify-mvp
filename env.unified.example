# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# n8n Webhook URLs
# Legacy webhook for empty room only (optional - for backward compatibility)
EXPO_PUBLIC_N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/empty-room

# NEW: Unified processing webhook (processes both empty room + style in one call)
EXPO_PUBLIC_N8N_UNIFIED_WEBHOOK_URL=https://your-n8n-instance.com/webhook/process-room-unified

# n8n Authentication (if using basic auth)
N8N_WEBHOOK_AUTH_USERNAME=your_username
N8N_WEBHOOK_AUTH_PASSWORD=your_password

# AI Services
OPENAI_API_KEY=your_openai_api_key

# Optional: Analytics and other services
MIXPANEL_TOKEN=your_mixpanel_token
AMPLITUDE_API_KEY=your_amplitude_key

# Development
EXPO_PUBLIC_ENV=development

# Supabase Edge Function Environment Variables (for deployment)
# These should be set in your Supabase dashboard under Edge Functions > Settings
N8N_UNIFIED_WEBHOOK_URL=https://your-n8n-instance.com/webhook/process-room-unified
SUPABASE_URL_HOST=your-project.supabase.co 