{
  "name": "homify",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "clean": "rm -rf node_modules/.cache",
    "reset": "npm run clean && npx expo start --clear",
    "restart": "npm run clean && expo start -c",
    "prod": "expo start --no-dev",
    "fix-deps": "expo install --check",
    "clear-cache": "rm -rf /tmp/metro-* && rm -rf node_modules/.cache",
    "optimize-images": "node scripts/optimize-images.js",
    "analyze-bundle": "npx expo export --platform web --dev false",
    "performance-test": "npm run clean && npm run optimize-images && expo start --no-dev",
    "build-profile": "eas build --profile development --platform all",
    "build-production": "eas build --profile production --platform all",
    "setup-env": "node scripts/setup-env.js",
    "test-env": "node scripts/test-environment.js",
    "test-webhook": "node scripts/test-webhook.js",
    "test-edge-function": "node scripts/test-edge-function.js",
    "test-rn-network": "node scripts/test-react-native-network.js",
    "test-n8n-payload": "node scripts/test-n8n-payload.js",
    "test-fixed-payload": "node scripts/test-fixed-payload.js",
    "setup-storage": "node scripts/setup-supabase-storage.js",
    "check-storage": "node scripts/check-existing-storage.js",
    "test-http-url": "node scripts/test-http-url-approach.js",
    "test-unified": "node scripts/test-unified-workflow.js",
    "test-rn-simple": "node scripts/test-rn-network-simple.js",
    "test-rooms-bucket": "node scripts/test-rooms-bucket.js",
    "create-rooms-bucket": "node scripts/create-rooms-bucket.js",
    "test-edge-n8n": "node scripts/test-edge-function-n8n-call.js",
    "test-complete-flow": "node scripts/test-complete-upload-flow.js",
    "test-simple-upload": "node scripts/test-simple-edge-upload.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@supabase/supabase-js": "2.38.4",
    "dotenv": "^16.5.0",
    "expo": "53.0.13",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.9",
    "expo-constants": "~17.1.6",
