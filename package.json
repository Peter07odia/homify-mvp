{
  "name": "homify",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules/.cache",
    "reset": "npm run clean && npx expo start --clear",
    "restart": "npm run clean && expo start -c",
    "prod": "expo start --no-dev",
    "fix-deps": "expo install --check",
    "clear-cache": "rm -rf /tmp/metro-* && rm -rf node_modules/.cache",
    "optimize-images": "node scripts/optimize-images.js",
    "analyze-bundle": "npx expo export --platform web --dev false",
    "performance-test": "npm run clean && npm run optimize-images && expo start --no-dev",
    "build-profile": "eas build --profile development --platform all",
    "build-production": "eas build --profile production --platform all"
  },
  "dependencies": {
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@supabase/supabase-js": "2.38.4",
    "expo": "^53.0.9",
    "expo-blur": "~14.1.4",
    "expo-camera": "~16.1.6",
    "expo-file-system": "~18.1.10",
    "expo-haptics": "~14.1.4",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-media-library": "~17.1.6",
    "expo-status-bar": "~2.2.3",
    "metro": "0.82.0",
    "metro-config": "0.82.0",
    "metro-resolver": "0.82.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-dotenv": "3.4.9",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-url-polyfill": "2.0.0",
    "react-native-vision-camera": "^4.6.4",
    "expo-dev-client": "~5.1.8",
    "expo-constants": "~17.1.6",
    "expo-image": "~2.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@react-native-community/cli": "^18.0.0",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
